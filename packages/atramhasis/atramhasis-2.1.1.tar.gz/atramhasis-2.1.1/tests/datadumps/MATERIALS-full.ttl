    skos:broader <https://id.erfgoed.net/thesauri/materialen/32> ;
    skos:inScheme <https://id.erfgoed.net/thesauri/materialen> ;
    skos:prefLabel "amber"@nl-BE ;
    skos:scopeNote "Amber is een fossiele hars afkomstig uit naaldbomen en wordt gebruikt voor het maken van kralen, hangers, knopen en dergelijke meer."@nl-BE .

<https://id.erfgoed.net/thesauri/materialen/35> a skos:Concept ;
    dcterms:identifier 35 ;
    dcterms:source [ a dcterms:BibliographicResource ;
            dcterms:bibliographicCitation "Nederlandse Art &amp; Architecture Thesaurus, <a href=\"http://browser.aat-ned.nl/\">http://browser.aat-ned.nl/</a>"^^rdf:HTML ] ;
