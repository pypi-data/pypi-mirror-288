    skos:inScheme <https://id.erfgoed.net/thesauri/materialen> ;
    skos:prefLabel "bont"@nl-BE ;
    skos:scopeNote "Bont is een gelooide dierlijke huid, dicht bezet met haren. Het wordt voornamelijk gebruikt voor het maken van kleding."@nl-BE .

<https://id.erfgoed.net/thesauri/materialen/25> a skos:Concept ;
    dcterms:identifier 25 ;
    dcterms:source [ a dcterms:BibliographicResource ;
            dcterms:bibliographicCitation "Nederlandse Art &amp; Architecture Thesaurus, <a href=\"http://browser.aat-ned.nl/\">http://browser.aat-ned.nl/</a>"^^rdf:HTML ] ;
