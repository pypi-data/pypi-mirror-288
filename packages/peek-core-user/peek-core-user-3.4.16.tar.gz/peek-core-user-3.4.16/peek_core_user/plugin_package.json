{
    "plugin": {
        "title": "Users",
        "packageName": "peek_core_user",
        "version": "3.4.16",
        "buildNumber": "#PLUGIN_BUILD#",
        "buildDate": "#BUILD_DATE#",
        "creator": "Synerty Pty Ltd",
        "website": "www.synerty.com"
    },
    "requiresServices": [
        "logic",
        "storage",
        "field",
        "office",
        "admin",
        "agent",
        "worker",
        "admin-doc",
        "office-doc"
    ],
    "admin-doc": {
        "docDir": "admin-doc",
        "docRst": "index.rst",
        "hasApi": true
    },
    "office-doc": {
        "docDir": "both-doc",
        "docRst": "index.rst"
    },
    "field": {
        "showHomeLink": true,
        "homeLinkText": "User",
        "appModule": "plugin-user.module#PluginUserModule",
        "appDir": "_private/both-app",
        "moduleDir": "plugin-module",
        "assetDir": "_private/both-assets",
        "icon": "/assets/peek_core_user/plugin_icon.png",
        "rootServices": [
            {
                "file": "services/logged-in.guard",
                "class": "LoggedInGuard"
            },
            {
                "file": "services/logged-out.guard",
                "class": "LoggedOutGuard"
            },
            {
                "file": "services/user.service",
                "class": "UserService",
                "persistent": true
            },
            {
                "file": "_private/user-tuple.service",
                "class": "UserTupleService",
                "persistent": true
            }
        ]
    },
    "office": {
        "showHomeLink": false,
        "appDir": "_private/both-app",
        "moduleDir": "plugin-module",
        "assetDir": "_private/both-assets",
        "icon": "/assets/peek_core_user/plugin_icon.png",
        "rootServices": [
            {
                "file": "services/logged-in.guard",
                "class": "LoggedInGuard"
            },
            {
                "file": "services/logged-out.guard",
                "class": "LoggedOutGuard"
            },
            {
                "file": "services/user.service",
                "class": "UserService",
                "persistent": true
            },
            {
                "file": "_private/user-tuple.service",
                "class": "UserTupleService",
                "persistent": true
            }
        ]
    },
    "admin": {
        "homeLinkText": "Users",
        "moduleDir": "plugin-module",
        "appDir": "_private/admin-app",
        "appModule": "user.module#UserModule"
    },
    "logic": {},
    "agent": {},
    "worker": {},
    "storage": {
        "alembicDir": "_private/alembic"
    }
}
