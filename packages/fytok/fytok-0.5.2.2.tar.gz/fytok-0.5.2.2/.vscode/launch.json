  "version": "0.2.0",
  "configurations": [
    {
      "name": "Python: Current File",
      "type": "debugpy",
      "request": "launch",
      "program": "${file}",
      "console": "integratedTerminal",
      "justMyCode": false,
      "cwd": "${workspaceFolder}/../output",
      "env": {
        "PYTHONPATH": "${workspaceFolder}/python:${workspaceFolder}/../spdm/python:${workspaceFolder}/../fytok_ext/python"
      }
    }
  ]
}
