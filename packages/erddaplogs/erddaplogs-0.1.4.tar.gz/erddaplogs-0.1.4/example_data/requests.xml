<dataset type="EDDTableFromAsciiFiles" datasetID="requests" active="true">
    <reloadEveryNMinutes>10080</reloadEveryNMinutes>
    <updateEveryNMillis>10000</updateEveryNMillis>
    <fileDir>/path/to/files/</fileDir>
    <fileNameRegex>.*anonymized_requests.csv</fileNameRegex>
    <recursive>true</recursive>
    <pathRegex>.*</pathRegex>
    <metadataFrom>last</metadataFrom>
    <standardizeWhat>0</standardizeWhat>
    <charset>ISO-8859-1</charset>
    <columnSeparator>,</columnSeparator>
    <columnNamesRow>1</columnNamesRow>
    <firstDataRow>2</firstDataRow>
    <sortedColumnSourceName>datetime</sortedColumnSourceName>
    <sortFilesBySourceNames>datetime</sortFilesBySourceNames>
    <fileTableInMemory>false</fileTableInMemory>
    <addAttributes>
        <att name="cdm_data_type">Other</att>
        <att name="Conventions">COARDS, CF-1.10, ACDD-1.3</att>
        <att name="infoUrl">https://github.com/callumrollo/erddaplogs</att>
        <att name="institution">Your Institution Here</att>
        <att name="keywords">browser, BrowserFamily, bytes, bytes_sent, code, data, dataset, dataset_id, dataset_type, datetime, device, DeviceFamily, erddap, erddap_request_type, family, file, file_type, identifier, local, request, sent, source, status, status_code, taxonomy, time, type</att>
        <att name="license">[standard]</att>
        <att name="sourceUrl">(local files)</att>
        <att name="standard_name_vocabulary">CF Standard Name Table v70</att>
        <att name="summary">Usage data for this ERDDAP server- anonymised requests</att>
        <att name="testOutOfDate">now-1day</att>
        <att name="title">requests</att>
    </addAttributes>
    <dataVariable>
        <sourceName>ip_id</sourceName>
        <destinationName>ip_id</destinationName>
        <dataType>String</dataType>
        <addAttributes>
            <att name="ioos_category">Identifier</att>
            <att name="long_name">Ip Id</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>datetime</sourceName>
        <destinationName>time</destinationName>
        <dataType>String</dataType>
        <addAttributes>
            <att name="ioos_category">Time</att>
            <att name="long_name">Datetime</att>
            <att name="source_name">datetime</att>
            <att name="standard_name">time</att>
            <att name="time_precision">1970-01-01T00:00:00.000Z</att>
            <att name="units">yyyy-MM-dd&#39;T&#39;HH:mm:ss.SSS000</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>url</sourceName>
        <destinationName>url</destinationName>
        <dataType>String</dataType>
        <addAttributes>
            <att name="ioos_category">Unknown</att>
            <att name="long_name">Url</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>status_code</sourceName>
        <destinationName>status_code</destinationName>
        <dataType>short</dataType>
        <addAttributes>
            <att name="_FillValue" type="short">32767</att>
            <att name="ioos_category">Unknown</att>
            <att name="long_name">Status-code</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>bytes_sent</sourceName>
        <destinationName>bytes_sent</destinationName>
        <dataType>long</dataType>
        <addAttributes>
            <att name="_FillValue" type="int">9223372036854775807</att>
            <att name="ioos_category">Unknown</att>
            <att name="long_name">bytes_sent</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>erddap_request_type</sourceName>
        <destinationName>erddap_request_type</destinationName>
        <dataType>String</dataType>
        <addAttributes>
            <att name="ioos_category">Unknown</att>
            <att name="long_name">Erddap Request Type</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>dataset_id</sourceName>
        <destinationName>dataset_id</destinationName>
        <dataType>String</dataType>
        <addAttributes>
            <att name="ioos_category">Identifier</att>
            <att name="long_name">Dataset Id</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>file_type</sourceName>
        <destinationName>file_type</destinationName>
        <dataType>String</dataType>
        <addAttributes>
            <att name="ioos_category">Unknown</att>
            <att name="long_name">File Type</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>dataset_type</sourceName>
        <destinationName>dataset_type</destinationName>
        <dataType>String</dataType>
        <addAttributes>
            <att name="ioos_category">Unknown</att>
            <att name="long_name">Dataset Type</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>BrowserFamily</sourceName>
        <destinationName>BrowserFamily</destinationName>
        <dataType>String</dataType>
        <addAttributes>
            <att name="ioos_category">Taxonomy</att>
            <att name="long_name">Browser Family</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>DeviceFamily</sourceName>
        <destinationName>DeviceFamily</destinationName>
        <dataType>String</dataType>
        <addAttributes>
            <att name="ioos_category">Taxonomy</att>
            <att name="long_name">Device Family</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>OS</sourceName>
        <destinationName>OS</destinationName>
        <dataType>String</dataType>
        <addAttributes>
            <att name="ioos_category">Unknown</att>
            <att name="long_name">OS</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>base_url</sourceName>
        <destinationName>base_url</destinationName>
        <dataType>String</dataType>
        <addAttributes>
            <att name="ioos_category">Unknown</att>
            <att name="long_name">OS</att>
        </addAttributes>
    </dataVariable>
    <dataVariable>
        <sourceName>request_kwargs</sourceName>
        <destinationName>request_kwargs</destinationName>
        <dataType>String</dataType>
        <addAttributes>
            <att name="ioos_category">Unknown</att>
            <att name="long_name">OS</att>
        </addAttributes>
    </dataVariable>
</dataset>

